{
    "name": "datashaman/laravel-teams",
    "description": "",
    "keywords": [
        "laravel",
        "teams"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "^5.8",
        "illuminate/contracts": "^5.8",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datashaman\\Teams\\TeamsServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\Teams\\": "src/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Datashaman\\Teams\\Tests\\": "tests/"
        }
    }
}
